mylist = ["bread", "bagel", "simit", "cookies"]
print(mylist)
index_var = len(mylist) // 2
newlist = mylist[0:index_var]
print(newlist)
del mylist[:index_var]
print(mylist)
mylist.extend(newlist)
print(mylist)

n = int(input("Enter a single digit integer : ")) 
# iterate between 0 to entered number 
for i in range(n): 
    # check if number is even and print it 
    if(i%2 == 0): 
        print(i)
